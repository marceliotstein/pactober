/**
 * This file should get overwritten if Method 1 is used. If Method 2 is chosen,
 * remove this comment and start adding your styles to this file.
 */

.tcenter {
  height: 100%;
  width: 100%;
  text-align:center;
  vertical-align: middle;
}

#first-time {
  display: none;
}

h1 { 
  font-family: 'Montserrat', sans-serif;
  font-size: 1.8em;
}

.big-title-well h1 { 
  font-family: 'Montserrat', sans-serif;
  font-size: 2.5em;
}

.little-logo-well {
  text-align: center;
}

.little-title-well {
  padding-top: 20px;
  text-align: center;
}

.big-logo-well {
  text-align: center;
}

.big-title-well {
  height: 200px;
  text-align: center;
  vertical-align: middle;
}

.dropdown-menu>li {
  text-align: right;
  border-bottom: 2px;
}

.page-header {
  border-bottom: 0px;
}

@media (min-width: 768px) {
  .container-fluid {
    padding: 8px;
  }
}

@media (max-width: 768px) {
  .container-fluid {
    padding: 18px;
  }
}

.pac-border {
  border-style: solid;
  border-color: black;
  border-width: 1px;
  border-radius: 9px;
}

.pac-logobar {
  text-align: center;
  padding: 18px;
}

.pac-topbar {
  text-align: left;
  padding: 9px 0px 9px 0px;
  text-color: #336699;
}

.btn-default {
  color: #336699;
}

.btn-default a,
.btn-default a.link,
.btn-default a.visited,
.btn-default a.active {
  color: #336699;
  text-decoration: none;
}

.pac-large {
  font-size: 2.1em;
  line-height: 36px;
}

.pac-medium {
  font-size: 1.2em;
  line-height: 24px;
}

.view-pac-discussions {
  background-color: green;
  width: 300px;
  padding: 9px;
  margin: 9px;
  text-color: #ffffff;
  float: left;
}

.view-pac-stories {
  background-color: blue;
  width: 300px;
  padding: 9px;
  margin: 9px;
  text-color: #ffffff;
  float: left;
}

.pac-mainwrap {
  padding: 9px 0px 9px 0px;
}
  
.pac-rightbar {
  padding: 9px 9px 9px 9px;
  background-color: #ffffff;
  height: 500px;
  border-width: 1px;
  border-color: #2f2f2f;
  border-style: solid;
  border-radius: 9px;
}

.pac-core {
  padding: 9px 9px 9px 9px;
}

table {
  border-collapse: separate;
  border-spacing: 18px;
}

.field-name-taxonomy-forums {
  padding-bottom: 9px;
}

.jumbotron {
  margin-right: 18px;
  height: 27px;
  background-color: #ffffff; 
  padding: 27px 0px 54px 0px;
}

.jumbotron h2 {
  font-size: 45px;
}

.pacadd {
  padding-top: 18px;
}

.leftnavname a,
.leftnavname a.link,
.leftnavname a.visited,
.leftnavname a.active {
  color: #5f5f5f;
  text-decoration: none;
}

.leftnavlogo {
  padding-bottom: 18px;
}

.leftnavblurb {
  padding-right: 9px;
}

.chimptime {
  text-align: left;
}

#mc_embed_signup {
  width: 200px;
}

.leftnav {
  margin: 9px 0px 18px 9px;
  padding: 9px;
  border-style: dotted;
  border-width: 0px;
  border-right-width: 5px;
  border-radius: 5px;
  border-right-color: #afafaf;
  background-color: #ffffff;
}

.well {
  border-width: 0px;
  background-color: #ffffff;
  box-shadow: inset 0 0px 0px;
}
